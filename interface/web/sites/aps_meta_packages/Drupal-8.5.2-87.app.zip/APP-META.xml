	<global-settings/>
	<upgrade match="/application/version &gt;= '8.0' and /application/release &gt;= '1'"/>
	<service id="drupal">
		<license must-accept="true">
			<text>
				<name>GNU GENERAL PUBLIC LICENSE</name>
				<file>htdocs/LICENSE.txt</file>
			</text>
		</license>
		<presentation>
			<name>Drupal</name>
			<summary>Use Drupal to build everything from personal blogs to enterprise applications. Thousands of add-on modules and designs let you build any site you can imagine.</summary>
			<infolinks>
				<link class="support" href="https://www.drupal.org/support">Support
				</link>
			</infolinks>
			<entry-points>
				<entry dst="/">
					<label>Site</label>
				</entry>
				<entry class="control-panel" dst="/user/login" method="POST">
					<label>Administrative interface</label>
					<variable class="login" name="name" value-of-setting="admin_name"/>
					<variable class="password" name="pass" value-of-setting="admin_password"/>
					<variable name="APS">ON</variable>
				</entry>
			</entry-points>
		</presentation>
		<settings>
			<group class="authn">
				<name>Administrator's preferences</name>
				<setting class="login" default-value="admin" id="admin_name" max-length="32" min-length="1" regex="^[a-zA-Z][0-9a-zA-Z_\-]*" type="string">
					<name>Administrator's login</name>
					<name xml:lang="de-DE">Login administratora</name>
					<name xml:lang="fr-FR">La connexion de l'administrateur</name>
					<name xml:lang="es-ES">Inicio de sesión de administrador</name>
					<name xml:lang="pl-PL">Login administratora</name>
					<error-message>Please make sure the text you entered starts with a letter and continues with either numbers, letters, underscores or hyphens.</error-message>
					<error-message xml:lang="de-DE">Bitte stellen Sie sicher, dass der von Ihnen eingegebene Text beginnt mit einem Buchstaben und fährt entweder mit Zahlen, Buchstaben, Unterstriche oder Bindestriche.</error-message>
					<error-message xml:lang="fr-FR">S'il vous plaît assurez-vous que le texte que vous avez entré commence par une lettre et se poursuit avec des nombres, des lettres, des caractères de soulignement ou des tirets.</error-message>
					<error-message xml:lang="es-ES">Please make sure the text you entered starts with a letter and continues with either numbers, letters, underscores or hyphens.</error-message>
					<error-message xml:lang="pl-PL">Upewnij sie, ze wpisany tekst zaczyna sie od litery i nadal zarówno z cyfr, liter, podkreslen i myslników.</error-message>
				</setting>
				<setting class="password" id="admin_password" type="password">
					<name>Password</name>
					<name xml:lang="de-DE">Kennwort</name>
					<name xml:lang="fr-FR">mot de passe</name>
					<name xml:lang="es-ES">contraseña</name>
					<name xml:lang="pl-PL">haslo</name>
				</setting>
			</group>
			<group class="vcard">
				<name>Main configuration</name>
				<group class="email">
					<setting class="value" default-value="test@test.com" id="admin_email" type="email">
						<name>Administrator's email</name>
						<name xml:lang="de-DE">Administrator E-Mail</name>
						<name xml:lang="fr-FR">L'e-mail de l'administrateur</name>
						<name xml:lang="es-ES">Correo electrónico del administrador</name>
						<name xml:lang="pl-PL">E-mail administratora</name>
					</setting>
				</group>
				<group class="fn n">
					<setting class="title" default-value="Drupal" id="title" min-length="1" type="string">
						<name>Site name</name>
						<name xml:lang="de-DE">Site name</name>
						<name xml:lang="fr-FR">Nom du site</name>
						<name xml:lang="es-ES">nombre del sitio</name>
						<name xml:lang="pl-PL">Nazwa strony</name>
					</setting>
				</group>
			</group>
		</settings>
		<requirements>
			<php:version min="5.5.9" xmlns:php="http://apstandard.com/ns/1/php"/>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">dom</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">filter</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">gd</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">hash</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">json</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">mysqli</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">mysqlnd</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">pcre</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">PDO</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">session</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">SimpleXML</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">SPL</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">tokenizer</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">openssl</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">xml</php:extension>
			<php:extension xmlns:php="http://apstandard.com/ns/1/php">mbstring</php:extension>
			<php:memory-limit xmlns:php="http://apstandard.com/ns/1/php">134217728</php:memory-limit>
			<php:max-execution-time xmlns:php="http://apstandard.com/ns/1/php">30</php:max-execution-time>
			<db:db xmlns:db="http://apstandard.com/ns/1/db">
				<db:id>main</db:id>
				<db:default-name>drupal</db:default-name>
				<db:can-use-tables-prefix>true</db:can-use-tables-prefix>
				<db:server-type>mysql</db:server-type>
				<db:server-min-version>5.5.3</db:server-min-version>
			</db:db>
		</requirements>
		<provision>
			<url-mapping>
				<default-prefix>drupal</default-prefix>
				<installed-size>88783917</installed-size>
				<mapping path="htdocs" url="/">
					<php:handler xmlns:php="http://apstandard.com/ns/1/php">
						<php:extension>php</php:extension>
					</php:handler>
					<mapping url="tmp">
						<php:permissions writable="true" xmlns:php="http://apstandard.com/ns/1/php"/>
					</mapping>
					<mapping url="sites/default/files">
						<php:permissions writable="true" xmlns:php="http://apstandard.com/ns/1/php"/>
					</mapping>
				</mapping>
			</url-mapping>
			<configuration-script name="configure">
				<script-language>php</script-language>
			</configuration-script>
		</provision>
	</service>
</application>
