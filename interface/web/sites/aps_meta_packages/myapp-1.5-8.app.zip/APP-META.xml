    		</choice>
    		<choice id="shop_silver">
    			<name>Shop (Silver)</name>
    			<name xml:lang="fr-FR">Boutique (Silver)</name>
    		</choice>
    		<choice id="shop_gold">
    			<name>Shop (Gold)</name>
    			<name xml:lang="fr-FR">Boutique (Gold)</name>
    		</choice>
