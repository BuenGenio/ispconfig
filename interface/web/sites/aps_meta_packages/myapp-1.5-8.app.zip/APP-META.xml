    	<setting type="string" id="sskey" default-value="00000000">
    		<name>Secure key</name>
    		<name xml:lang="fr-FR">Clef de sécurité</name>
    		<description>Access key to the service (used during provisionning).</description>
    		<description xml:lang="fr-FR">Clef d'accés au service (utilisée pendant la création d'un compte).</description>
    	</setting>
