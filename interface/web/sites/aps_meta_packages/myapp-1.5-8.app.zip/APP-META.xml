	    			<description xml:lang="fr-FR">Rue (adresse) du compte utilisateur.</description>
	    		</setting>
	    		<setting type="string" id="street_address_2" track-old-value="true" default-value="" class="extended-address">
	    			<name>Street address (ligne 2)</name>
	    			<description>Street address of the user.</description>
	    			<description xml:lang="fr-FR">Rue (adresse) du compte utilisateur - ligne 2</description>
	    		</setting><setting type="string" id="city" default-value="" track-old-value="true" class="locality">
