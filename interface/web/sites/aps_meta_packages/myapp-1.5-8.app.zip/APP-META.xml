	    			<description>City of the user.</description>
	    			<description xml:lang="fr-FR">Ville du compte utilisateur.</description>
	    		</setting>
	    		<setting type="string" id="zip_code" default-value="" track-old-value="true" class="postal-code">
	    			<name>Zip</name>
	    			<name xml:lang="fr-FR">Code postal</name>
	    			<description>Zip code of the user.</description>
	    			<description xml:lang="fr-FR">Code postal du compte utilisateur.</description>
	    		</setting>
	    		<setting type="string" id="country" default-value="France" track-old-value="true" class="country-name">
