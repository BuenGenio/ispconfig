    <global-settings>
    	<setting type="string" id="username_api" default-value="">
    		<name>Acces login to the API</name>
    		<name xml:lang="fr-FR">Login d'accès à l'API</name>
    		<description>User (admin) login to acces the API</description>
    		<description xml:lang="fr-FR">Login utilisateur (compte administrateur) d'accès à l'API</description>
    	</setting>
    	<setting type="password" id="password_api" default-value="">
    		<name>Access password to the API</name>
    		<name xml:lang="fr-FR">Mot de passe d'accés à l'API</name>
    		<description>Access password (admin account) to the API.</description>
    		<description xml:lang="fr-FR">Mot de passe (du compte administrateur) d'accès à l'API</description>
    	</setting>
    	<setting type="string" id="url_api" default-value="">
    		<name>API URL</name>
    		<name xml:lang="fr-FR">URL d'accès à l'API</name>
    		<description>API's url.</description>
    		<description xml:lang="fr-FR">URL d'accès à l'API</description>
    	</setting>
    	<setting type="string" id="reseller_id" default-value="" visibility="hidden">
