    	<settings>
			<group class="authn">    		
	    		<setting type="string" id="user_mail" uniq="global" default-value="" class="email" visibility="hidden" track-old-value="true">
	    			<name>User Email</name>
	    			<name xml:lang="fr-FR">Email du compte utilisateur</name>
