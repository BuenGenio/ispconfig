    <service class="service" id="IASO" singular="true">
        <license must-accept="true">
            <text>
                <name>End User IASO Backup License Agreement</name>
                <file>License.txt</file>
            </text>
        </license>
        <presentation>
            <name>IASO Backup Management</name>
            <summary>IASO Backup Application</summary>
            <infolinks>
                <link class="official" href="https://www.iaso.com/">Official site</link>
                <link class="support" href="https://www.iaso.com/support">Support</link>
                <link class="deployment-guide" href="https://www.iaso.com/kb/article.php?url=IASOARTICLE40">Deployment guide</link>
            </infolinks>
            <entry-points>
                <entry dst="https://www.iaso.com/downloads">
                    <label>Download IASO Backup Client</label>
                </entry>
                <entry dst="https://www.iaso.com/">
                    <label>Information service</label>
                </entry>
                <entry dst="https://www.iaso.com/technical-support/">
                    <label>Support</label>
                    <icon path="images/icon.png"/>
                </entry>
            </entry-points>
        </presentation>
        
        <settings>
            <group class="vcard">
                <group class="fn n">
                    <setting class="login" id="GroupAccountName" installation-only="true" type="string" uniq="global">
                        <name>Group Account Name</name>
                        <description>Group Account Name</description>
                    </setting>
                </group>
                <setting class="password" default-value="password" id="GroupAccountPwd" installation-only="true" type="password">
                    <name>Group Account Password</name>
                    <description>Password</description>
                </setting>
                <setting class="email" id="groupemail" installation-only="true" type="string">
                    <name>E-mail</name>
                    <description>Should be a real e-mail address to receive notifications</description>
                </setting>
            </group>
            <group class="company">
                <setting class="dbsize" default-value="1" id="dbsize" type="integer" visibility="hidden"/>
            </group>
            <setting class="subscription_id" default-value="" id="subscription_id" protected="true" type="string" visibility="hidden">
                <name>Subscription ID</name>
            </setting>
            <setting class="subscription_trial" default-value="true" id="subscription_trial" protected="true" type="string" visibility="hidden">
                <name>Subscription Trial</name>
            </setting>
        </settings>
        <resources>
            <resource class="mb" id="dbsize" limiting-setting="dbsize">
                <name>Used Storage resource</name>
                <description>Total storage size resource</description>
            </resource>
        </resources>
        
        <requirements>
            <php:version min="5.1.2" xmlns:php="http://apstandard.com/ns/1/php"/>
            <php:extension xmlns:php="http://apstandard.com/ns/1/php">libxml</php:extension>
            <php:extension xmlns:php="http://apstandard.com/ns/1/php">SOAP</php:extension>
            <php:extension xmlns:php="http://apstandard.com/ns/1/php">XMLWriter</php:extension>
        </requirements>
        
        <provision>
            <configuration-script name="configure.php">
                <script-language>php</script-language>
                <status-control/>
                <structured-output/>
            </configuration-script>
            <resource-script name="resources.php" poll-interval="60m">
                <script-language>php</script-language>
            </resource-script>
        </provision>
        <service class="account" id="account">
            <presentation>
                <name>IASO Backup Account</name>
                <summary>IASO Backup Application</summary>
                <infolinks>
                    <link class="official" href="https://www.iaso.com/">Official site</link>
                    <link class="support" href="https://www.iaso.com/technical-support/">Support</link>
                </infolinks>
                <entry-points>
                    <entry class="Download" dst="https://www.iaso.com/downloads" method="GET">
                        <label>Download IASO Backup Client</label>
                    </entry>
                    <entry dst="https://www.iaso.com/">
                        <label>Information service</label>
                    </entry>
                    <entry dst="https://www.iaso.com/technical-support/">
                        <label>Support</label>
                        <icon path="images/icon.png"/>
                    </entry>
                </entry-points>
            </presentation>
            <settings>
                <setting id="subscription_id2" protected="true" type="string" value-of-setting="subscription_id" visibility="hidden"/>
                <setting id="groupemail" protected="true" type="string" value-of-setting="groupemail" visibility="hidden"/>
                <group class="vcard">
                    <setting id="GroupAccountName2" type="string" value-of-setting="GroupAccountName" visibility="hidden">
                        <name>Group Account Name</name>
                        <description>Group Account Name</description>
                    </setting>
                    <setting id="GroupAccountPwd2" type="string" value-of-setting="GroupAccountPwd" visibility="hidden">
                        <name>Group Account Password</name>
                        <description>Group Account Password</description>
                    </setting>
                    <setting class="login" id="AccountName" installation-only="true" min-length="1" regex="^[a-zA-Z0-9_@\.-]*$" type="string" visibility="hidden">
                        <name>Account login</name>
                        <description>Should be a real e-mail address to receive
                            notifications</description>
                        <error-message>The following characters may be used: [a-z] or
                            [A-Z] or [0-9] or '-' or '.' or '_'or '@'. A point is a
                            prohibited symbol at the end of the account name</error-message>
                    </setting>
                    <setting class="password" id="AccountPwd" regex="^[^\s,:;'&quot;/|\\]{1,251}$" type="string">
                        <name>Password</name>
                        <description>Password</description>
                        <error-message>The following characters are invalid: 
                            spaces or quotes or ':' or ',' or ';' or '\'or '|' or '/'.</error-message>
                    </setting>
                    <setting default-value="" id="License-Code" protected="true" type="string">
                        <name>License code</name>
                        <description>You'll get this value after provisioning.
                        </description>
                    </setting>
                    <setting default-value="" id="LicenseCode" protected="true" type="string" visibility="hidden">
                        <name>License code</name>
                    </setting>
                    <setting default-value="" id="DashboardEmail" regex="^[a-zA-Z0-9_@\.-]*$" type="string">
                        <name>Dashboard Email</name>
                        <description/>
                        <error-message>The following characters may be used: [a-z] or
                            [A-Z] or [0-9] or '-' or '.' or '_'or '@'.</error-message>
                    </setting>
                    <setting default-value="0" id="AccountLimit" type="integer">
                        <name>Account Limit</name>
                        <description>Enter value in GB. Use '0' value for unlimited
                            storage.</description>
                    </setting>
                    <setting default-value="" id="HiddenPassword" installation-only="true" protected="true" type="string">
                        <name>Password</name>
                        <description>Password</description>
                    </setting>
                    <setting default-value="Server" id="MachineType" type="enum">
                        <name>Machine Type</name>
                        <choice id="Workstation">
                            <name>Workstation</name>
                        </choice>
                        <choice id="Server">
                            <name>Server</name>
                        </choice>
                    </setting>
                </group>
            </settings>
