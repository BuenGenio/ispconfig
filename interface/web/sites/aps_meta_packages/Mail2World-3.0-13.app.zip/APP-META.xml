    	<requirements xmlns:php="http://apstandard.com/ns/1/php" xmlns:dns="http://apstandard.com/ns/1/dns">
    		<dns:record>
    			<dns:mx>
    				<dns:src>
    					<dns:external value-of-setting="user_domain"></dns:external>
    				</dns:src>
    				<dns:priority>10</dns:priority>
    				<dns:dst>
    					<dns:external value-of-setting="mx_smtp_server"></dns:external>
    				</dns:dst>
    			</dns:mx>
    		</dns:record>
    		<dns:record>
    			<dns:cname>
    				<dns:src>
    					<dns:external value-of-setting="domain_cname_records"></dns:external>
    				</dns:src>
    				<dns:dst>
    					<dns:external value-of-setting="domain_cname_targets"></dns:external>
    				</dns:dst>
    			</dns:cname>
    		</dns:record>
    		<dns:record>
    			<dns:id>mail</dns:id>
    			<dns:cname>
    				<dns:src>
    					<dns:external value-of-setting="mail_domain_cname_records"></dns:external>
    				</dns:src>
    				<dns:dst>
    					<dns:external value-of-setting="mail_server"></dns:external>
    				</dns:dst>
    			</dns:cname>
    		</dns:record>
    		<dns:record>
    			<dns:id>imap</dns:id>
    			<dns:cname>
    				<dns:src>
    					<dns:external value-of-setting="imap_domain_cname_records"></dns:external>
    				</dns:src>
    				<dns:dst>
    					<dns:external value-of-setting="imap_server"></dns:external>
    				</dns:dst>
    			</dns:cname>
    		</dns:record>
    		<dns:record>
    			<dns:id>pop</dns:id>
    			<dns:cname>
    				<dns:src>
    					<dns:external value-of-setting="pop_domain_cname_records"></dns:external>
    				</dns:src>
    				<dns:dst>
    					<dns:external value-of-setting="pop_server"></dns:external>
    				</dns:dst>
    			</dns:cname>
    		</dns:record>
    		<dns:record>
    			<dns:id>wap</dns:id>
    			<dns:cname>
    				<dns:src>
