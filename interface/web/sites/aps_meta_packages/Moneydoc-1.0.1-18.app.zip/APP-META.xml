     	<settings>
<group class="authn">
<setting class="password" id="PASSWORD" min-length="4" type="password">
<name>Mot de passe</name>
	<error-message>Veuillez saisir un mot de passe valide.</error-message>
</setting>
</group>
<group class="vcard">
<group class="email">
<setting class="value" id="EMAIL" installation-only="true" type="email">
<name>Email utilisateur</name>
	<error-message>Veuillez saisir un email valide.</error-message>
</setting>
</group>
</group>
<setting id="ACCOUNT_ID" optional="true" protected="true" type="string" uniq="global" visibility="hidden"/>
<setting default-value="true" id="STATUS" type="string" visibility="hidden"/>
<setting default-value="8" id="SUBSCRIPTION" type="enum" visibility="hidden">
              <name>Votre abonnement Moneydoc</name>
              <choice id="8">
                 <name>Moneydoc Pro Gratuit</name>
              </choice><choice id="9">
                 <name>Moneydoc Pro Basique</name>
              </choice>
              <choice id="10">
                 <name>Moneydoc Pro Premium</name>
              </choice>
           </setting>
</settings>
<resources>
            <resource class="item" id="subscription" limiting-setting="SUBSCRIPTION">
        <name>Subscription Type</name>
        <description>Subscription Type</description>
     </resource>
 </resources>
   	<requirements xmlns:php="http://apstandard.com/ns/1/php">
   		<php:extension>openssl</php:extension>
   		<php:extension>curl</php:extension>
   		<php:version min="5.3"/>
   	</requirements>
   	<provision>
   		<configuration-script name="configure-script.php">
   			<script-language>php</script-language>
   			<structured-output/>
   		</configuration-script>
   	</provision>
   	<service class="service" id="moneydoc_access" singular="true">
   		<presentation>
   			<name>Moneydoc</name>
   			<summary>Votre service moneydoc</summary>
   			<entry-points>
   			<entry class="login" dst="https://pro.moneydoc.fr/" method="POST">
   				<label>Moneydoc Pro Application</label>
   				<variable class="login" name="login" value-of-setting="EMAIL"/>
   				<variable class="password" name="password" value-of-setting="PASSWORD"/>
   			</entry>
   		</entry-points>
   		</presentation>
   		<settings>
   			<setting id="ACCOUNT_ID" protected="true" type="string" value-of-setting="ACCOUNT_ID" visibility="hidden"/>
   			<setting id="STATUS" type="string" value-of-setting="STATUS" visibility="hidden"/>
               <setting id="EMAIL" type="string" value-of-setting="EMAIL" visibility="hidden"/>
               <setting id="PASSWORD" type="string" value-of-setting="PASSWORD" visibility="hidden"/>
   		</settings>
   		<requirements xmlns:php="http://apstandard.com/ns/1/php">
   		</requirements>
   	</service>
   </service>
